}

@article{liu_machine_2020,
	title = {Machine learning interatomic potential developed for molecular simulations on thermal properties of $\beta$-{Ga2O3}},
	volume = {153},
	issn = {0021-9606, 1089-7690},
	shorttitle = {{GAP} for $\beta$},
	url = {https://pubs.aip.org/jcp/article/153/14/144501/316601/Machine-learning-interatomic-potential-developed},
	doi = {10.1063/5.0027643},
	abstract = {The thermal properties of $\beta$-Ga2O3 can significantly affect the performance and reliability of high-power electronic devices. To date, due to the absence of a reliable interatomic potential, first-principles calculations based on density functional theory (DFT) have been routinely used to probe the thermal properties of $\beta$-Ga2O3. DFT calculations can only tackle small-scale systems due to the huge computational cost, while the thermal transport processes are usually associated with large time and length scales. In this work, we develop a machine learning based Gaussian approximation potential (GAP) for accurately describing the lattice dynamics of perfect crystalline $\beta$-Ga2O3 and accelerating atomic-scale simulations. The GAP model shows excellent convergence, which can faithfully reproduce the DFT potential energy surface at a training data size of 32 000 local atomic environments. The GAP model is then used to predict ground-state lattice parameters, coefficients of thermal expansion, heat capacity, phonon dispersions at 0 K, and anisotropic thermal conductivity of $\beta$-Ga2O3, which are all in excellent agreement with either the DFT results or experiments. The accurate predictions of phonon dispersions and thermal conductivities demonstrate that the GAP model can well describe the harmonic and anharmonic interactions of phonons. Additionally, the successful application of our GAP model to the phonon density of states of a 2500-atom $\beta$-Ga2O3 structure at elevated temperature indicates the strength of machine learning potentials to tackle large-scale atomic systems in long molecular simulations, which would be almost impossible to generate with DFT-based molecular simulations at present.},
	language = {en},
	number = {14},
	urldate = {2025-12-19},
	journal = {The Journal of Chemical Physics},
	author = {Liu, Yuan-Bin and Yang, Jia-Yue and Xin, Gong-Ming and Liu, Lin-Hua and Cs{\'a}nyi, G{\'a}bor and Cao, Bing-Yang},
	month = sep,
	year = {2020},
	pages = {144501},
	file = {??:/Users/amphilagusgu/Zotero/storage/XB2H3M82/Liu ? - 2020 - Machine learning interatomic potential developed for molecular simulations on thermal properties of.pdf:application/pdf},
}

@article{sun_neuroevolution_2023,
	title = {A neuroevolution potential for predicting the thermal conductivity of \textbf{ \textit{$\alpha$} } , \textbf{ \textit{$\beta$} } , and \textbf{ \textit{$\varepsilon$} } -{Ga2O3}},
